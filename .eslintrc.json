{
  "env": {
      "browser": true,
      "es2021": true,
      "jest": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "import/extensions": [2, "always"]
  }
}
